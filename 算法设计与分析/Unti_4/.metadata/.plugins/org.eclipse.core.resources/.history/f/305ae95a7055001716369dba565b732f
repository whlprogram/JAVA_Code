package knapsack;

import java.util.Scanner;

public class Knapsack {

	public static void main(String[] args) {
		// 3 20 18 15 10 25 24 15
		Scanner scan = new Scanner(System.in);		
		int n = scan.nextInt();//物品个数n		
		int c = scan.nextInt();//背包容量c		
		int w[] = new int[n+1];//每个物品重量wi
		w[0] = 0;
		for(int i=1; i<=n; i++)
			w[i] = scan.nextInt();
		int v[] = new int[n+1];//每个物品价值vi
		v[0] = 0;
		for(int i=1; i<=n; i++)
			v[i] = scan.nextInt();
		float x[] = new float[n+1];//每个物品价值vi
		x[0] = (float)0.0;
		for(int i=1; i<=n; i++)
			x[i] = (float)v[i]/w[i];
		for(int i=1; i<=n; i++)
			System.out.print(x[i] + " ");

	}
