package test;

import java.util.Scanner;

public class test1 {

	public static void main(String[] args) {
		// TODO 自动生成的方法存根
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		System.out.println(F(n));
	}
	public static int F(int n){
		int f0=1, f1=1, f2=0;
		for(int i=2; i<=n; i++){
			f2 = f0 + f1;
			f0 = f1;
			f1 = f2;
		}
		return f2;
	}

}
